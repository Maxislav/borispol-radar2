@import "../../styl/constant.styl"
.ukbb-component
  height 100%
  background white
  .title
    padding 15px 5px  0 5px
    color $color-text
  .img-container
    width 675px
    height 480px
    padding 5px
    &>div
      height 100%
  img
    box-sizing border-box
    width 100%
    height 100%
  .img-ukbb
    margin 0 auto
    height 100%
    display block
  .err-mess
    position absolute
    left 10px
    top 10px
    color red
    text-shadow 1px 1px 10px black
  .text-info
    position absolute
    left 10px
    top 10px
